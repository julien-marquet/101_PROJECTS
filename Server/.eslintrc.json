{
    "extends": "airbnb-base",
    "rules": {
        "import/no-dynamic-require": [
            0
        ],
        "no-underscore-dangle": [
            0
        ],
        "global-require": [
            0
        ],
        "max-len": [
            0
        ],
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ]
    }
}